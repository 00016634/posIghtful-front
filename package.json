{
  "name": "posightful-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "tw:build": "tailwindcss --input src/styles.css --output src/styles.generated.css",
    "tw:watch": "tailwindcss --input src/styles.css --output src/styles.generated.css --watch",
    "start": "npm run tw:build && concurrently \"npm run tw:watch\" \"ng serve\"",
    "build": "npm run tw:build && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@9.2.0",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/pwa": "^21.0.1",
    "@angular/router": "^21.0.0",
    "@angular/service-worker": "^21.0.2",
    "@tailwindcss/postcss": "^4.1.18",
    "angularx-qrcode": "^21.0.4",
    "date-fns": "^4.1.0",
    "echarts": "^6.0.0",
    "lucide-angular": "^0.563.0",
    "ngx-echarts": "^21.0.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.18",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "21.1.2",
    "@angular/cli": "21.1.2",
    "@angular/compiler-cli": "21.1.2",
    "@angular/localize": "21.1.2",
    "@tailwindcss/cli": "^4.1.18",
    "concurrently": "^9.2.1",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
